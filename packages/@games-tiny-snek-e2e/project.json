{
	"name": "@games-tiny-snek-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/@games-tiny-snek-e2e/src",
	"projectType": "application",
	"targets": {
		"e2e": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"cypressConfig": "packages/@games-tiny-snek-e2e/cypress.config.ts",
				"devServerTarget": "@games-tiny-snek:serve",
				"testingType": "e2e"
			},
			"configurations": {
				"production": {
					"devServerTarget": "@games-tiny-snek:serve:production"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/@games-tiny-snek-e2e/**/*.{js,ts}"]
			}
		}
	},
	"tags": [],
	"implicitDependencies": ["@games-tiny-snek"]
}
